{
  "name": "@jenkins-cd/react-flow",
  "version": "0.0.25",
  "description": "Generic code to create a Flow with react",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run start-storybook -- -p 9001",
    "build-storybook": "build-storybook --static-dir ./node_modules/@jenkins-cd/design-language/dist/assets/ --output-dir website-build",
    "start-storybook": "start-storybook --static-dir ./node_modules/@jenkins-cd/design-language/dist/assets/",
    "storybook:port": "npm run start-storybook -- -p ",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha --compilers js:babel-register --require babel-polyfill ./.storybook/__conf__/mochaMockConfig.js \"./stories/*-spec.jsx\"",
    "prepublish": "babel src --out-dir dist"
  },
  "repository": "https://github.com/scherler/blueocean-react-flow",
  "author": "Thorsten Scherler",
  "license": "MIT",
  "dependencies": {
    "@jenkins-cd/design-language": "0.0.158",
    "@jenkins-cd/logging": "0.0.6",
    "@kadira/storybook": "2.35.3",
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "mobx": "2.6.0",
    "mobx-react": "3.5.7",
    "mobx-utils": "1.1.2",
    "prop-types": "15.5.10",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "2.8.1"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "2.5.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "expect": "1.20.2",
    "jsdom": "9.11.0",
    "mocha": "3.4.1",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.4.2",
    "storybook-addon-specifications": "1.0.16"
  }
}
